{
  "authenticationMethod": "none",
  "routes": [
    {
      "authenticationType": "none",
      "csrfProtection": false,
      "source": "^/Northwind/(.*)$",
      "destination": "Northwind",
      "target": "$1"
    },
    {
        "source": "^/zlib/mylibrary/(.*)$",
        "target": "$1",
		"localDir": "../mylibrary/src"
	},    
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt"
    }
  ]
}